{
  "manifest_version": 3,
  "name": "Click Maps Easy",
  "version": "1.0",
  "description": "Make the map div clickable for Google Maps searches.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
